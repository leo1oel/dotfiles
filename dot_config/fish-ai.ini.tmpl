[fish-ai]
configuration = google
completions = 8
temperature = 0.2
history_size = 5
keymap_1 = ctrl-p
keymap_2 = ctrl-\\

[local-llama]
provider = self-hosted
server = http://localhost:11434/v1
model = llama3.1:8b

[google]
provider = google
api_key = {{ secret "api-key/google" "api-key-was-not-set" }}
